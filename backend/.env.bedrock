# Environment configuration for Bedrock-only mode
# Copy this to .env to use only AWS Bedrock models

# MongoDB Configuration
mongo_db_url=mongodb://localhost:27017/

# AWS Configuration
bucket_name=your-s3-bucket-name
AWS_REGION=us-east-1

# Bedrock Configuration - ENABLE BEDROCK ONLY
USE_BEDROCK_ONLY=true

# Optional: Leave these empty when using Bedrock only
openai_api_key=
gemini_api_key=
gcp_project_id=
aws_access_key_id=
aws_secret_access_key=